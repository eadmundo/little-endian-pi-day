{
  "name": "little-endian-pi-day",
  "version": "0.0.1",
  "description": "A React app that counts down to Pi day expressed in little-endian date format",
  "repository": {
    "type": "git",
    "url": "https://github.com/eadmundo/little-endian-pi-day"
  },
  "main": "app/main.js",
  "scripts": {
